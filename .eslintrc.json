{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018
  },
  "plugins": [],
  "rules": {
    "no-console": [0],
    "max-classes-per-file": [0],
    "consistent-return": [1],
    "implicit-arrow-linebreak": [0],
    "comma-dangle": [0],
    "indent": [0],
    "object-curly-spacing": [0],
    "no-multiple-empty-lines": [0]
  }
}
